{
  "name": "grocery-list",
  "version": "1.0.0",
  "description": "app to follow your grocery list",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "NODE_ENV=test jest",
    "start": "./node_modules/.bin/dotenvx run --env-file=.env.production -- node index.js",
    "dev": "./node_modules/.bin/dotenvx run --env-file=.env.development -- node-dev index.js"
  },
  "author": "Virginie Lemaire",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^0.37.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint": "^8.47.0",
    "express": "^4.21.2",
    "jest": "^29.6.2",
    "joi": "^17.9.1",
    "pg": "^8.7.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/settings/"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/settings/setup.js"
    ]
  },
  "devDependencies": {
    "node-dev": "^8.0.0"
  }
